  "private": false,
  "scripts": {
    "bump": "npm install && npm test && npm version patch && git push && git push --tags && npm publish --access public",
    "eslint": "node node_modules/eslint/bin/eslint **/*.js ",
    "mocha": "node node_modules/mocha/bin/_mocha",
